{
  "expo": {
    "name": "Careerhouse",
    "slug": "careerhouse",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": [
        "NOTIFICATIONS"
      ]
    },
    "plugins": [
      "expo-notifications"
    ]
  }
}
